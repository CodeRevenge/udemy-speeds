{
  "manifest_version": 2,
  "name": "Udemy Speeds",
  "version": "1.2",
  "description": "Muestra más opciones de velocidad de reproducción en Udemy.",
  "icons": {
    "48": "icons/udemy-48.png",
    "196": "icons/udemy-196.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.udemy.com/*"],
      "js": ["udemy_speed.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "48.0"
    }
  }
}
